package com.eheinen.main;

import com.eheinen.exec_01.SavingAccount;

public class Main {

	public static void main(String args[]){
		// Exercise 1:
		SavingAccount savingAccount = new SavingAccount("Eduardo Heinen", "Paulista Avenue", "000.000.000-00");
		boolean isEquals = savingAccount.equals("000.000.000-00");
		System.out.println(isEquals);
		
		isEquals = savingAccount.equals("000.000.000-01");
		System.out.println(isEquals);
	}
}
